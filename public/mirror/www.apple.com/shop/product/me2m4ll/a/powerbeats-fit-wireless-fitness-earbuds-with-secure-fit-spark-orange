        <script id="shldVerify" type="module" crossorigin="anonymous" src="/shop/shld/v1/verify.js" integrity="sha384-ks5BFNp9vK3a6D/0EnZWJmYLnvFIKqx9dh3gsx67abkqD22O3pf42MkW90VxN8TP"></script>
